<% include ./partials/header %>
<link rel="stylesheet" href="/stylesheet/Fefe.css">
<div class="container text-align-center" style="margin-top:5%">
  <div class="row align-center ">
    <div class="col-1"></div>
    <div class="col-10">
    <form  id="realform"action="/register" method="POST"></form>
      <button class="btn btn-secondary" style="width:auto;margin-bottom:1px" type ="button" data-toggle="collapse" data-target="#regisStd">
        <label>ข้อมูลส่วนตัว</label>
      </button>
      <div class="card">
        <div id="regisStd" class="collapse show">
          <div class="card-body">
            <form class="form-in" style="background-color:#F7F7F7">
            <div class="form-group row">
              <div class="col-sm-12 col-from-label text-center">
                <img src="https://i.imgur.com/OKiZmaC.jpg?1" alt="studentPic" style="width:256px;height:256px">
              </div>
            </div>
            <div class="form-group row">
              <label class="col-sm-1 col-form-label text-nowrap" style="margin-right:20px">คำนำหน้าชื่อ:</label>
              <div class="col-sm-2">
                <input form ="realform" type="text" class="form-control" name='titleTeacher' value="<%= tinfo[0].titlename%>" disabled>
              </div>
              <label class="col-sm-1 col-form-label text-nowrap" >ชื่อ:</label>
              <div class="col-sm-3">
                <input form ="realform" type="text" class="form-control" name='firstnameTeacher' value="<%= tinfo[0].firstname%>" disabled>
              </div>
              <label class="col-sm-1 col-form-label text-nowrap" style="margin-right:10px">นามสกุล:</label>
              <div class="col-sm-3">
                <input form ="realform" type="text" class="form-control" name='lastnameTeacher' value="<%= tinfo[0].lastname%>" disabled>
              </div>
            </div>
            <div class="form-group row">
              <label class="col-sm-1 col-form-label text-nowrap">อีเมลล์:</label>
              <div class="col-sm-4">
                <input form ="realform" type="text" class="form-control" name='emailTeacher' value="<%= tinfo[0].email%>" disabled>
              </div>
              <label class="col-sm-1 col-form-label text-nowrap" style="margin-right:20px">เบอร์โทรศัพท์:</label>
              <div class="col-sm-2">
                <input form ="realform" type="text" class="form-control" name='phoneTeacher' value="<%= tinfo[0].tel%>" disabled>
              </div>
              <label class="col-sm-1 col-form-label text-nowrap">เพศ:</label>
              <div class="col-sm-2">
                <input form ="realform" type="text" class="form-control" name='genderTeacher' value="<%= tinfo[0].gender%>" disabled>
              </div>
            </div>

          </div>
        </div>
      </div>
<% include ./partials/footer %>
