<% include ./partials/header %>


<div class="container text-align-center" style="margin-top:3%">
     <div class="row">
       <div class="col-3">
       </div>
       <div class="col-6">
       <h2>จำนวนนักศึกษาที่ลงทะเบียนเรียนในวิชาที่กำหนด ของแต่ละภาควิชา</h2>
         <table class="table table-bordered">
           <thead class="thead-light">
             <tr>
               <th>รหัสวิชา</th>
               <th>ภาควิชา</th>
               <th>จำนวน</th>
             </tr>
           </thead>
           <% subject.forEach(function(s){ %>
           <tbody class="table-light text-center">
               <tr>
                 <th><%= s.subjectid %> </th>
                 <th><%= s.dept %> </th>
                 <th><%= s.count %> </th>
               </tr>
           </tbody>
           <% }); %>
         </table>
       </div>
       <div class="col-3">
       </div>
     </div>
   </div>

<% include ./partials/footer %>
